@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ex: <http://example.com/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix activity: <http://example.com/activity/> .
@prefix semantify: <http://semantify.it/> .
@prefix schema: <http://schema.org/> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix ttg: <http://ttg.at/> .
@base <http://sti2.at/> . #the base for the classes


<#LOGICALSOURCE>
rml:source "./examples/test.json";
rml:referenceFormulation ql:JSONPath;
rml:baseSource <#LodgingMapping>;
rml:iterator "$.*".


<#LodgingMapping>
rml:logicalSource <#LOGICALSOURCE>;

rr:subjectMap [
    rr:template "{[@id]}"; #the id of the object
    rr:class schema:LodgingBusiness;
];

rr:predicateObjectMap [
    rr:predicate schema:name;
    rr:objectMap [ rml:reference "[http://schema.org/name]" ];
];
rr:predicateObjectMap [
    rr:predicate schema:description;
    rr:objectMap [ rml:reference "[http://schema.org/description]" ];
];

rr:predicateObjectMap [
   rr:predicate schema:geo;
   rr:objectMap  [
      rr:parentTriplesMap <#GEOmapping>;

   ];
].

<#GEOmapping>
rml:logicalSource <#LOGICALSOURCE>;

  rr:subjectMap [
    rr:termType rr:BlankNode;
    rr:class schema:GeoCoordinates;
 ];

 rr:predicateObjectMap [
     rr:predicate schema:latitude;
     rr:objectMap [ rml:reference "[http://schema.org/geo].[http://schema.org/latitude]" ];
  ];


 rr:predicateObjectMap [
    rr:predicate schema:longitude;
    rr:objectMap [ rml:reference "[http://schema.org/geo].[http://schema.org/longitude]" ];
  ].

