@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix prefix: <http://mytestprefix.org/> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@base <http://sti2.at/> . #the base for the classes


<#LOGICALSOURCE>
rml:source "./tests/replaceMapping/input.json";
rml:referenceFormulation ql:JSONPath;
rml:iterator "$".

<#LOGICALSOURCE2>
rml:source "./tests/replaceMapping/input.json";
rml:referenceFormulation ql:JSONPath;
rml:iterator "$.theSports".


<#Mapping>
rml:logicalSource <#LOGICALSOURCE>;

 rr:subjectMap [
    rr:termType rr:BlankNode;
    rr:class prefix:Person;
 ];


rr:predicateObjectMap [
    rr:predicate prefix:name;
    rr:objectMap [ rml:reference "name" ];
];

rr:predicateObjectMap [
    rr:predicate prefix:age;
    rr:objectMap [ rml:reference "age" ];
];

rr:predicateObjectMap [
    rr:predicate prefix:likesSports;
    rr:objectMap  [
           rr:parentTriplesMap <#SPORTSmapping>;
        ];
].

<#SPORTSmapping>
rml:logicalSource <#LOGICALSOURCE>;

 rr:subjectMap [
    rr:template "prefix:{sports.id}";
    rr:class prefix:Sport;
 ].

<#CLASSIFIERMapping>
rml:logicalSource <#LOGICALSOURCE2>;

 rr:subjectMap [
    rr:template "prefix:{id}"; #the id of the object
    rr:class prefix:Sport;
 ];

 rr:predicateObjectMap [
     rr:predicate prefix:sports;
     rr:objectMap [ rml:reference ".[School,Private]" ];
 ];

rr:predicateObjectMap [
    rr:predicate prefix:description;
    rr:objectMap [ rml:reference "description" ];
].






